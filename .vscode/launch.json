{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Alim Terminal Extension",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/extension.js",
      "runtimeExecutable": "${execPath}",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}"
      ],
      "outFiles": [
        "${workspaceFolder}/out/**/*.js"
      ],
      "preLaunchTask": "",
      "sourceMaps": true,
      "internalConsoleOptions": "openOnSessionStart"
    }
  ]
}
